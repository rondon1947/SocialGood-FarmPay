<h1 style="text-align:center;"><em>ORDER</em> SUMMARY</h1>
<div class="row container" style="margin: 2% 5%;">
  <div style="background-color: rgb(128, 131, 133);" class="col-md-6">
    <header style="text-align: left;"><h1>Requested Orders:</h1></header>




    <table class="table table-dark table-striped table-light table-hover">
      <tbody>
      <tr>
        <th scope="row">Wholesaler</th>
        <td>Ramlal</td>
      </tr>
      <tr>
        <th scope="row">Crop</th>
        <td>Wheat</td>
      </tr>
      <tr>
        <th scope="row">Price Offered</th>
        <td>100</td>
      </tr>
      <tr>
        <th scope="row">Quantity</th>
        <td>1 quintal</td>
      </tr>




      </tbody>
    </table>

    <form>
      <input style="width:25%; background-color: rgb(71, 206, 71); text-align: center; align-content: left;" type="submit" value="Accept">
      <input style="width:25%; background-color: rgb(218, 42, 42); text-align: center; float: right;" type="submit" value="Decline">
    </form>


  </div>
  <div style="background-color: rgb(79, 82, 79);"class="col-md-6 column">
    <header style="text-align: right;color: aliceblue;"><h1>Stock Fill Request:</h1></header>


    <table class="table table-dark table-striped table-hover">
      <tbody>
      <tr>
        <th scope="row">Crop</th>
        <td>Rice</td>
      </tr>
      <tr>
        <th scope="row">Quantity</th>
        <td>1 quintal</td>
      </tr>
      <tr>
        <th scope="row">Placed Date</th>
        <td>10 March'21</td>
      </tr>
      <tr>
        <th scope="row">Status</th>
        <td>
          <button class="btn btn-danger disabled" style="padding: 10px 15px;" ></button>
          <button class="btn btn-warning  disabled" style="padding: 10px 15px;"></button>
          <button class="btn btn-success disabled" style="padding: 10px 15px;"></button>
        </td>
      </tr>
      <tr>
        <th scope="row">Accepted By</th>
        <td>Kishore</td>
      </tr>




      </tbody>
    </table>

  </div>
</div>

